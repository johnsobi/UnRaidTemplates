<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <TemplateURL>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/deluge.xml</TemplateURL>
  <Beta>False</Beta>
  <Category>Cloud, Productivity</Category>
  <Name>LogicalDoc CE</Name>
  <Description></Description>
  <Overview>
  </Overview>
  <Support>https://forums.logicaldoc.com/</Support>
  <Registry>https://hub.docker.com/r/logicaldoc/logicaldoc-ce/</Registry>
  <GitHub></GitHub>
  <Repository>logicaldoc/logicaldoc-ce</Repository>
  <Project>https://www.logicaldoc.com/</Project>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
    <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>      
    </Publish>
  </Networking>
  <Environment>
    <Variable>
      <Name>LDOC_MEMORY</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_ENGINE</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_PORT</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_NAME</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_USER</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_PASSWORD</Name>
      <Value></Value>
    </Variable>
    <Variable>
      <Name>DB_HOST</Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/opt/logicaldoc/conf</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/opt/logicaldoc/repository</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <Icon>https://raw.githubusercontent.com/binhex/docker-templates/master/binhex/images/deluge-icon.png</Icon>  
</Containers>